{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Python: Flask",
        "type": "python",
        "request": "launch",
        "module": "flask",
        "env": {
          "FLASK_APP": "${workspaceFolder}/MyWeb.py",
          "FLASK_ENV": "development",
          "FLASK_RUN_EXTRA_FILES": "${workspaceFolder}/app",
          "FLASK_RUN_PORT": "5000"
        },
        "jinja": true,
        "args": [
            "run",
            "--debugger",
            "--no-reload"
        ]
      }
    ]
  }
  